{
  "name": "musefuze-studios",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "nodemon server/index.js",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview --port 4173 --host",
    "build:production": "npm run lint && vite build",
    "serve": "vite preview --port 3000 --host",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "server:prod": "NODE_ENV=production node server/index.js",
    "test:cors": "curl -H \"Origin: http://localhost:3000\" -H \"Access-Control-Request-Method: POST\" -H \"Access-Control-Request-Headers: X-Requested-With\" -X OPTIONS http://localhost:5000/api/test-cors",
    "test:cors:prod": "curl -H \"Origin: https://musefuzestudios.com\" -H \"Access-Control-Request-Method: POST\" -H \"Access-Control-Request-Headers: X-Requested-With\" -X OPTIONS https://api.musefuzestudios.com/api/test-cors",
    "deploy:frontend": "npm run build:production && netlify deploy --prod --dir=dist",
    "deploy:preview": "npm run build && netlify deploy --dir=dist",
    "build:full": "npm run build:production && npm run build:server",
    "build:server": "node scripts/build-server.js",
    "start:production": "NODE_ENV=production node dist-server/index.js",
    "deploy:vps": "npm run build:full && npm run package:deployment",
    "package:deployment": "node scripts/package-deployment.js",
    "setup:production": "node scripts/setup-production.js",
    "diagnose:mysql": "node scripts/diagnose-mysql.js",
    "migrate:db": "node scripts/migrate-role.js",
    "start": "NODE_ENV=production node server/index.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.18",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "fs-extra": "^11.3.0",
    "globals": "^15.9.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.0",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
